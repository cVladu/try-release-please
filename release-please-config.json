{
  "bootstrap-sha": "73fc6349431e4c45dd29f0559a40298cef2fe56a",
  "release-type": "simple",
  "group-pull-request-title-pattern": "auto: release ${version}",
  "include-v-in-tag": true,
  "include-component-in-tag": true,
  "packages": {
    ".": {
      "include-component-in-tag": false,
      "version-file": "version.txt",
      "pull-request-header": "Automated release", 
      "pull-request-footer": " "
    },
    "docker-action": {
      "path": "docker-action",
      "component": "docker-action", 
      "version-file": "version.txt"
    },
    "hello-action": {
      "path": "hello-action",
      "component": "hello-action", 
      "version-file": "version.txt"
    },
    "hello-composite": {
      "path": "hello-composite",
      "component": "hello-composite",
      "version-file": "version.txt"
      "last-release-sha": "1a40bfbd8419d491e0106f2bb97267cec0293ad6"
    }
  }
}
